<div class="container">
  <div class="row justify-content-center">
    <div class="col-6 m-2">
      <%= simple_form_for [ @course, @occurence ] do |f| %>
          <%= f.input :date, input_html: {class: "datepicker"} %>
          <%= f.input :price %>
          <%= f.input :capacity %>
          <%= f.input :location %>
          <%= f.submit class: "btn btn-outline-success" %>
      <% end %>
    </div>
  </div>
</div>
